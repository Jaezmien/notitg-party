<ActorFrame
	InitCommand="sleep,9e9"
><children>
	<Layer
		Type="Quad"
		InitCommand="%function(self)
			MESSAGEMAN:Broadcast('PartyGameplayInit')
		end"
		OnCommand="queuecommand,Stop"
		StopCommand="%function(self)
			GAMESTATE:SetSongBeat(0)
			SCREENMAN:GetTopScreen():PauseGame(true)

			MESSAGEMAN:Broadcast('PartyGameplayReady')
			PARTY_CMD:GameplayReady()
		end"
	/>
</children></ActorFrame>
